{
  "name": "mitinfy",
  "displayName": "mitinfy",
  "description": "music player integrated into vscode with low resource consumption",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mitinfy.init"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Mitinfy.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "Mitinfy.login",
        "title": "login into mitinfy"
      }
    ],
    "configuration": {
      "title": "Mitinfy",
      "properties": {
        "mitinfy.accessToken": {
          "type": "string",
          "default": "",
          "description": "Spotify access token",
          "scope": "machine"
        },
        "mitinfy.refreshToken": {
          "type": "string",
          "default": "",
          "description": "Spotify refresh token",
          "scope": "machine"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "express": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "spotify-web-api-node": "^5.0.2",
    "Typescript": "npm:typescript@^5.9.2",
    "express": "^5.1.0"
  },
  "author": "",
  "license": "ISC"
}
